<keymap version="1" name="Default copy" parent="$default">
  <action id="$Copy">
    <keyboard-shortcut first-keystroke="ctrl insert" />
  </action>
  <action id="ActivateTerminalToolWindow">
    <keyboard-shortcut first-keystroke="alt f12" />
    <keyboard-shortcut first-keystroke="alt c" />
  </action>
  <action id="CopyAsRichText">
    <keyboard-shortcut first-keystroke="ctrl c" />
  </action>
  <action id="EditorCopy">
    <keyboard-shortcut first-keystroke="ctrl insert" />
  </action>
  <action id="QuickDocCopy">
    <keyboard-shortcut first-keystroke="ctrl insert" />
  </action>
  <action id="XDebugger.CopyValue">
    <keyboard-shortcut first-keystroke="ctrl insert" />
  </action>
  <action id="com.eslint">
    <keyboard-shortcut first-keystroke="alt x" />
  </action>
</keymap>